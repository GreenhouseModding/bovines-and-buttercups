{
    "schemaVersion": 1,
    "id": "bovinesandbuttercups",
    "version": "${version}",
    "name": "Bovines and Buttercups",
    "description": "A mod that aims to provide a fresh take on the Moobloom mob from 2020's Mob Vote.",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "license": "MIT",
    "icon": "assets/modid/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
          "net.merchantpug.bovinesandbuttercups.BovinesAndButtercupsFabric"
        ]
    },
    "mixins": [
        "bovinesandbuttercups.mixins.json",
        "bovinesandbuttercups.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14.11",
        "fabric": "*",
        "minecraft": [
            "1.20", "1.20.1"
        ],
        "java": ">=17"

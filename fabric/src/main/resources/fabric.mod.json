{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "Bovines and Buttercups",
    "description": "A fresh, new take on the 2020 Mob Vote's Moobloom.",
    "authors": [
        "Greenhouse Team",
        "MerchantPug"
    ],
    "contributors": [
        "SpiderKolo"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/bovines-and-buttercups",
        "issues": "https://github.com/MerchantPug/bovines-and-buttercups/issues",
        "sources": "https://github.com/MerchantPug/bovines-and-buttercups"
    },
    "license": "${license}",
    "icon": "assets/bovinesandbuttercups/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "net.merchantpug.bovinesandbuttercups.BovinesAndButtercupsFabric"
        ],
        "rdpr": [
            "net.merchantpug.bovinesandbuttercups.registry.BovinesReloadableRegistryPlugin"
        ]
    },
    "custom": {
        "cardinal-components": [
            "bovinesandbuttercups:lockdown",
            "bovinesandbuttercups:moobloom_target",
            "bovinesandbuttercups:mooshroom_type"
        ]
    },
    "mixins": [
        "bovinesandbuttercups.mixins.json",
        "bovinesandbuttercups.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.15",
        "fabric-api": "*",
        "minecraft": "1.20.2",
        "java": ">=17"
    }
}